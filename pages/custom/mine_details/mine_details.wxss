/* pages/custom/mine_details/mine_details.wxss */
page {
  background-color: #E5E5E5;
  overflow-y: auto;
}

.container{
	width: 100%;
	height: 100%;
  overflow-x: hidden;
  
  overflow-y: hidden;
}
.container::-webkit-scrollbar {
  display: none;
}
.m_schedule{
  width: calc(100% - 21px);
  height: 175px;
  margin: auto;
  margin-top: 11px;
  margin-bottom: 11px;
  background-color: #ffffff;
  border-radius: 10px;
}
.m_information{
  width: calc(100% - 21px);
  /* min-height: 126px;
  max-height: 168px; */
  margin: auto;
  margin-bottom: 11px;
  background-color: #ffffff;
  border-radius: 10px;
}
.m_contact{
  width: calc(100% - 21px);
  height: 70px;
  margin: auto;
  background-color: #ffffff;
  border-radius: 10px;
  display: flex;
  /* justify-content: center;  */
  align-items: center;
  /* border: 1px solid darkcyan; */
}

.m_Order_information{
  width: 100%;
  height: auto;
  min-height: calc(100vh - 400px);
  margin: auto;
  margin-top:10px ;
  background: rgba(255, 255, 255, 0.8);
  border-radius: 40px 40px 0px 0px;
}
.order_content{
  width: 100%;
  height: 46px;
  line-height: 46px;
  padding-top: 15px;
}
.o_con_left_icon{
  width: calc(100% -324px);
  height: 46px;
  display: flex;
  align-items: center;
  margin-left: 32px;
  margin-right:20px;
  float: left;
}
.o_con_left_icon image{
  width: 30px;
  height: 30px;
  display: inline-block;
}
.o_con_right_number{
  width: 100%;
  height: 46px;
}
.o_con_right_title{
  font-family: AlibabaPuHuiTi;
  font-size: 16px;
  font-weight: normal;
  line-height: 25px;
  color: #000000;
}
.o_con_right_id{
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 14px;
  font-weight: normal;
  line-height: 25px;
  letter-spacing: 0px;
  font-feature-settings: "kern" on;
  color: #909090;
}
.flow_path{
  width: 100%;
  height: 14vh;
  /* border: 1px solid red; */
  margin: auto;
  position: relative;
}
.flow_path image{
  width: 100%;
  height: 70px;
  position: absolute;
  bottom: 0;
  margin-bottom: 10px;
}

.m_info_title{
  width: calc(100% - 176px);
  height: 25px;
  line-height: 25px;
  font-family: AlibabaPuHuiTi;
  font-size: 15px;
  font-weight: bold;
  letter-spacing: 0px;
  color: #000000;
  margin-left: 29px;
  padding-top: 17px;
}
.m_info_name{
  /* width: calc(100% - 141px); */
  /* height: 25px; */
  min-height: 25px;
  max-height: 40px;
  /* line-height: 25px; */
  font-size: 14px;
  margin-top: 10px;
  margin-left: 29px;
  margin-right: 10px;
  font-family: Alibaba PuHuiTi 2.0;
  font-weight: normal;
  letter-spacing: 0px;
  font-feature-settings: "kern" on;
  color: #000000;
}
.m_info_install{
  /* width: calc(100% - 243px); */
  margin-left: 29px;
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 14px;
  font-weight: normal;
  line-height: 25px;
  letter-spacing: 0px;
  font-feature-settings: "kern" on;
  color: #1A1A1A;
}
.m_info_install text{
  margin-right:10px;
  float: left;
}
.m_info_install_checkbox{
  margin-left:10px ;
}

.m_info_deliverytime,.m_info_installationtime{
  width: calc(100% - 49px);
  height: 24px;
  line-height: 24px;
  margin: auto;
  background: #FF8127;
  border-radius: 5px;
  position: relative;
}
.m_info_installationtime{
  margin-top: 3px;
}
.m_info_deliverytime text,.m_info_installationtime text{
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 14px;
  font-weight: 600;
  line-height: 25px;
  letter-spacing: 0px;
  font-variation-settings: "opsz" auto;
  font-feature-settings: "kern" on;
  color: #FFFFFF;
  margin-left: 4px;
}
.m_info_deliverytime image,.m_info_installationtime image{
  width: 16px;
  height: 16px;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 9px;
}
.m_contact_view{
  width: calc(100% - 100px);
  height: 41.18px;
  margin-left: 16.51px;
  /* border: 1px solid red; */
  margin-right:30.55px;
  /* display: flex; */
  /* align-items: center; */
}
.m_contact_img{
  width: 41.28px;
  height: 41.28px;
  background: #0256FF;
  border-radius: 50%;
  opacity: 1;
  float: left;
  display: flex;
  justify-content: center;
  align-items: center;
}
.m_contact_img image{
  width: 22px;
  height: 20px;
}

.m_contact_titel{
  width: calc(100% - 54px);
  height: 41.18px;
  line-height: 41.18px;
  /* border: 1px solid red; */
  margin-left: 54px;
}

.m_contact_name{
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 14px;
  font-weight: normal;
  font-feature-settings: "kern" on;
  color: #909090;
  float: left;
}
.m_contact_evaluate{
  height: 41.18px;
  line-height: 41.18px;
  display: flex;
  align-items: center; /* 垂直居中 */
  float: right;
  /* border: 1px solid red; */
}
.m_contact_evaluate text{
  width: 28px;
  /* border: 1px solid red; */
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 14px;
  font-weight: normal;
  line-height: 25px;
  text-align: center;
  letter-spacing: 0px;
  color: rgba(0, 0, 0, 0.5);
  float: left;
}
.m_contact_evaluate image{
  width: 14.45px;
  height: 14.41px;
}
.m_contact_phone{
  width: 34px;
  height: 51.32px;
  text-align: center; /* 水平居中 */
}
.m_contact_phone_img{
  width: 30.96px;
  height: 30.88px;
  border-radius: 50%;
  opacity: 1;
  /* background: #D8D8D8; */
  background: #FF8127;
  margin: auto;
  display: flex;
  align-items: center; /* 垂直居中 */
  justify-content: center; /* 水平居中 */
}
.m_contact_phone_img image{
  width: 75%;
  height: 75%;
  margin: auto;
}
.m_contact_phone text{
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 14px;
  font-weight: normal;
  display: inline-block;
  align-items: center;
  letter-spacing: 0px;
  color: rgba(0, 0, 0, 0.5);
}
.m_Order_information_but{
  width: calc(100% - 32px);
  height: 48px !important;
  padding-top: 25px;
  /* border: 1px solid red; */
  margin: auto;
  opacity: 1;
  /* padding-bottom: 16px; */
  margin-bottom: 16px;
}
.m_Order_information_but button{
  background: #F5F5F5;
  border-radius: 48px;
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 16px;
  font-weight: normal;
  line-height: 48px;
  text-align: center;
  align-items: center;
  letter-spacing: 0px;
  color: rgba(0, 0, 0, 0.5);
  /* border: 2px solid #F5F5F5; */
}
.m_Order_information_content{
  width: 90%;
  height: 150px;
  margin: auto;
  position: relative;
}
.m_Order_information_send{
  width: 28px;
  height: 28px;
  background: #D8D8D8;
  border-radius: 50%;
  float: left;
  display: flex;
  align-items: center; /* 垂直居中 */
  justify-content: center; /* 水平居中 */
  margin-top: 5px;
}
.m_Order_information_send image{
  width: 12px;
  height: 12px;
}
.m_Order_information_text{
  width: 90%;
  float: right;
}
.t_title{
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 14px;
  font-weight: bold;
  line-height: 28px;
  letter-spacing: 0px;
  font-variation-settings: "opsz" auto;
  color: rgba(0, 0, 0, 0.8);
}
.t_date{
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 15px;
  font-weight: normal;
  line-height: 26px;
  letter-spacing: 0px;
  color: rgba(0, 0, 0, 0.8);
}
.t_details{
  font-family: Alibaba PuHuiTi 2.0;
  font-size: 14px;
  font-weight: normal;
  line-height: 25px;
  letter-spacing: 0px;
  color: rgba(0, 0, 0, 0.8);
}
.header_step{
  width: calc(100% - 20px);
  height: 70px;
  margin: auto;
  margin-top: 26px;
}
.t-steps-item__line--horizontal{
  top: 22px !important;
}
.upload_image_content .t-icon, .t-icon--image{
  display: flex !important;
  justify-content: center !important;
  align-items: center !important;
}
.t-icon, .t-icon--image{
  width: 40px !important;
  height: 44px !important;
}
.t-steps-item__content--horizontal{
  margin-top: 2px !important;
}
.t-checkbox--block{
  padding: 0px !important;
  justify-content: center;
  align-items: center;
}
.t-checkbox{
  position:static !important;
}
.t-checkbox--block{
  display:inline-flex !important;
}
.t-button--default::after{
  border-color:#F5F5F5;
}
.t-button--default::after{
  border-color: #ffffff !important;
  border-top: #ffffff !important;
  border-right: #ffffff !important;
  border-bottom: #ffffff !important;
  border-left:#ffffff !important ;
}
.d_line{
  width: 1px;
  height: 120px;
  border-left: 1px dashed #dcdcdc;
  position: absolute;
  top: 33px;
}



