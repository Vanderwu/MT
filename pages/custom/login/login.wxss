page {
  background-color: #ffffff;
}
.container{
	width: 100%;
	height: 100%;
	display: flex;
	flex-direction: column;
	background-color: #ffffff;
  overflow-x: hidden;
  overflow-y: hidden;
}
.userinfo {
  width: 100%;
  height: 30vh;
  box-sizing: border-box;
  text-align: center;
  display: flex;
  justify-content: center; /* 水平居中 */
  align-items: center; /* 垂直居中 */
}
.userinfo_img{
  width: 25vw;
  height: 15vh;
  border: 1px solid rgb(163, 158, 158);
  background-size: cover; /* 图片等比例缩放并覆盖整个容器 */
  background-position: center; /* 图片居中显示 */
}

.emp_but{
  width: 100%;
  height: 10vh;
  display: flex;
  justify-content: center; /* 水平居中 */
  align-items: center; /* 垂直居中 */
}
.login_btn{
  width: 80%;
  height: 42px;
  line-height: 42px;
  background-color: #bbd3fb;
  color: #ffffff;
  border-radius: 30px;
}
.ordinance{
  width: 100%;
  height: 10vh;
  display: flex;
  justify-content: center;
  align-items: center;
}
.ordinance_check{
  width: 80%;
  height: 5vh;
  line-height: 5vh;
  font-size: 4vw;
}
.ordinance_prompt{
  width: 100%;
  height: 15vh;
  display: flex;
  justify-content: center;
  align-items: center;
}
.ordinance_text{
  width: 80%;
  font-size: 3.5vw;
}
.mt_logo{
width: 90%;
height: 10vh;
}


