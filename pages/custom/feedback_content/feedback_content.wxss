/* pages/custom/feedback_content/feedback_content.wxss *//* pages/custom/feedback/feedback.wxss */
page {
  background-color: #E5E5E5;
}

.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color: #ffffff;
  overflow-x: hidden;
  overflow-y: auto;
}
.container::-webkit-scrollbar {
  display: none;
}

.prompt {
  width: calc(100% - 40px);
  height: 125px;
  margin: auto;
  background: #EEEEEE;
  border-radius: 0px 0px 40px 40px;
  /* display: flex;
  justify-content: center;
  align-items: center; */
}


.content {
  width: calc(100% - 40px);
  height: calc(100vh - 125px - 80px - 10px);
  margin: auto;
}

.submit {
  width: 100%;
  height: 80px;
  background: #ffffff;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.submit .external-class {
  width: 90%;
  border-radius: 30px !important;
  background: rgb(5 100 245) !important;
  color: #ffffff !important;
  border-color: rgb(5 100 245);
}


/* .required::before {
  content: "*";
  color: red;
  margin-right: 5px;
} */

.t-input {
  padding: 12px !important;
}

.t-input__label {
  color: #646566 !important;
  font-family: Source Han Sans;
  font-size: 16px !important;
  font-weight: normal;
  line-height: normal;
}

.conProblem .t-input__label {
  font-family: AlibabaPuHuiTi;
  font-size: 17px;
  font-weight: bold;
  color: #000000;
}

t-input__control t-left t-class-input {
  color: 16px !important;
}

.category {
  width: 22%;
  font-family: Source Han Sans;
  font-size: 16px;
  font-weight: normal;
  line-height: normal;
  text-align: center;
  display: flex;
  align-items: center;
  letter-spacing: 0em;
  color: #646566;
  padding: 12px;
  float: left;
}

.radio {
  width: 71%;
  float: right;
}

/* .category::before {
  content: "*";
  color: red;
  margin-right: 5px;
} */

.radio_ {
  padding: 5px !important;
}

.t-radio__title {
  color: #B1B2B2 !important;
  font-family: Source Han Sans;
  font-size: 16px !important;
  font-weight: normal !important;
  line-height: normal;
}

.t-input__control {
  font-size: 16px !important;
  color: #646566 !important;
}

.con_category {
  height: 10vh;
}

.describe {
  width: 100%;
  font-family: Source Han Sans;
  font-size: 16px;
  font-weight: normal;
  line-height: normal;
  text-align: center;
  display: flex;
  align-items: center;
  letter-spacing: 0em;
  color: #646566;
  padding: 12px;
}

/* .describe::before {
  content: "*";
  color: red;
  margin-right: 5px;
} */

.external-class {
  width: 90%;
  height: 256rpx;
  border: 1px solid #000000;
  margin: auto;
  border-radius: 3px;
  /* background: #FFFFFF; */
  /* box-sizing: border-box; */
}

.billcode {
  width: 22%;
  font-family: Source Han Sans;
  font-size: 16px;
  font-weight: normal;
  line-height: normal;
  text-align: center;
  display: flex;
  align-items: center;
  letter-spacing: 0em;
  color: #646566;
  padding: 12px;
  float: left;
  margin: 10px;
}

.billcode::before {
  content: "*";
  color: #ffffff;
  margin-right: 5px;
}

.example-search {
  background-color: #fff;
  padding: 16rpx 32rpx;
}

.con_billcode {
  width: 100%;
  /* height: 8vh; */
  display: flex;
  align-items: center;
}

.up_name {
  font-family: Source Han Sans;
  font-size: 16px;
  font-weight: normal;
  line-height: normal;
  text-align: center;
  display: flex;
  align-items: center;
  letter-spacing: 0em;
  color: #646566;
}

.con_update{
  width: 100%;
  height: 8vh;
  margin-left: 10px;
  display: flex;
  align-items: center;
  /* border: 1px solid red; */
}
.con_update image {
  width: 47px;
  height: 47px;
}
.t-search__clear{
  display: none;
}
.t-search__input-box .t-input__keyword{
  font-size: 14px;
  font-weight: normal;
  line-height: 25px;
  text-align: center;
  letter-spacing: 0px;
  font-feature-settings: "kern" on;
  color: #909090 !important;
}
.t-button--default::after{
  border-color: #ffffff !important;
  border-top: #ffffff !important;
  border-right: #ffffff !important;
  border-bottom: #ffffff !important;
  border-left:#ffffff !important ;
}
.wrapper{
  margin-left:20px ;
}
.upload_image_content{
  width: 100%;
  min-height: 92px;
}

.wrapper .upload_image_content {
  width: 100% !important;
  height: 60px !important;
}

.t-grid-item{
  width: 60px !important;
  height: 60px !important;
}
.inputName{
  background: #000000 !important;
}

.header_step{
  width: 100%;
  height: 125px;
  display: flex;
}

.t-steps-item__line--horizontal{
  top: 50px !important;
}

.t-steps-item__content--horizontal{
  margin-top: 0px !important;
}

.header_step .t-icon, .t-icon--image{
  width: 40px !important;
  height: 44px !important;
}

/* .t-icon-base{
  display: none !important;
} */

.upload_image_content .t-icon, .t-icon--image{
  display: flex !important;
  justify-content: center !important;
  align-items: center !important;
}
